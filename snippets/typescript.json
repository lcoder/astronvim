	"react-memo-template": {
		"prefix": "react-memo-component",
		"body": [
			"import React, { memo } from 'react';",
			"import classNames from 'classnames';\n",
			"export interface ${TM_FILENAME_BASE}Props {",
			"  className?: string;",
			"}\n",
			"export const $TM_FILENAME_BASE = memo<${TM_FILENAME_BASE}Props>(function $TM_FILENAME_BASE(props) {",
			"  const {",
			"    className,",
			"  } = props;\n",
			"  return (",
			"    <div",
			"      className={classNames(className)}",
			"    >$0",
			"    </div>",
			"  );",
			"});\n"
		],
		"description": "tsx模板-函数式组件"
	},
		
	"react-hooks-template": {
		"prefix": "hooks",
		"body": [
			"\n",
			"export function ${TM_FILENAME_BASE}() {",
			"  $0",
			"}",
			"\n"
		]
	}
}
