{
	"react-fn-template": {
		"prefix": "react-fn-component",
		"body": [
			"import React from 'react';\n",
			"export interface ${TM_FILENAME_BASE}Props {\n  $0",
			"}\n",
			"export function $TM_FILENAME_BASE(props: ${TM_FILENAME_BASE}Props) {",
			"  const {} = props;\n",
			"  return (",
			"    <div>\n",
			"    </div>",
			"  );",
			"};\n"
		],
		"description": "tsx模板-函数式组件"
